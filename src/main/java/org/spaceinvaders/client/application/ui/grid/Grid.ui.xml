<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:b='urn:import:org.gwtbootstrap3.client.ui'
             xmlns:b.html='urn:import:org.gwtbootstrap3.client.ui.gwt'
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:with field="css" type="org.spaceinvaders.client.resources.AppResources"/>

    <ui:with type="org.spaceinvaders.client.place.NameTokens" field="nameTokens"></ui:with>

    <b:Panel>
        <b:PanelHeader>
            <b:Heading size="H3" text="Data Grid"/>
        </b:PanelHeader>
        <b:PanelBody>
            <b.html:DataGrid bordered="true" condensed="true" striped="true" hover="true" height="1000px"
                             width="100%" ui:field="dataGrid"/>
        </b:PanelBody>
    </b:Panel>
</ui:UiBinder>

