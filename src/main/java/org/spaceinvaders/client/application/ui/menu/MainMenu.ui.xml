<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:b='urn:import:org.gwtbootstrap3.client.ui'
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:with field="css" type="org.spaceinvaders.client.resources.AppResources"/>
    <ui:with type="org.spaceinvaders.client.place.NameTokens" field="nameTokens"/>

    <b:Navbar>
        <b:NavbarHeader>
            <b:NavbarBrand targetHistoryToken="{nameTokens.getHome}">Home page</b:NavbarBrand>
        </b:NavbarHeader>
        <b:NavbarNav ui:field="navBarNav">
            <b:ListDropDown>
                <b:AnchorButton dataToggle="DROPDOWN">Session</b:AnchorButton>
                <b:DropDownMenu ui:field="dropDownMenu"/>
            </b:ListDropDown>
        </b:NavbarNav>
    </b:Navbar>
</ui:UiBinder>