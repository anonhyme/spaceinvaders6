<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:b='urn:import:org.gwtbootstrap3.client.ui'
             xmlns:menu='urn:import:org.spaceinvaders.client.application.ui.menu'
             xmlns:graph='urn:import:org.spaceinvaders.client.application.ui.graph'>

    <g:HTMLPanel ui:field="main" >
        <menu:MainMenu/>
        <b:Progress>
            <b:ProgressBar ui:field="studentProgress" type="SUCCESS" percent="30"/>
        </b:Progress>
        <b:Progress>
            <b:ProgressBar ui:field="classProgress" type="WARNING" percent="40"/>
        </b:Progress>
        <b:Row>
            <b:Column size="MD_12" ui:field="col1"></b:Column>
            <b:Column size="MD_12" ui:field="col2"></b:Column>
            <b:Column size="MD_12" ui:field="col3"></b:Column>
            <b:Column size="MD_12" ui:field="col4"></b:Column>
            <b:Column size="MD_12" ui:field="col5"></b:Column>
            <b:Column size="MD_12" ui:field="col6"></b:Column>
        </b:Row>
    </g:HTMLPanel>

</ui:UiBinder>
