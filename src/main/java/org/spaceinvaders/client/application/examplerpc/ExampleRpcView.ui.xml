<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:menu='urn:import:org.spaceinvaders.client.application.ui.menu'
             xmlns:grid='urn:import:org.spaceinvaders.client.application.ui.grid'
             xmlns:b='urn:import:org.gwtbootstrap3.client.ui'
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:with field="css" type="org.spaceinvaders.client.resources.AppResources"/>
    <g:HTMLPanel>
        <menu:MainMenu/>
        <grid:Grid ui:field="grid"/>
        <b:Button ui:field="sendButton"/>
    </g:HTMLPanel>
</ui:UiBinder>
