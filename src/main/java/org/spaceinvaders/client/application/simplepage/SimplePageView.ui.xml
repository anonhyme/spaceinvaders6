<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:menu='urn:import:org.spaceinvaders.client.application.ui.menu'
             xmlns:b='urn:import:org.gwtbootstrap3.client.ui'
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:with field="css" type="org.spaceinvaders.client.resources.AppResources"/>

    <g:HTMLPanel>
        <div>
            <menu:MainMenu/>
        </div>

        <div class="row text-center">

            <div class="col-md-12">
                <g:Label ui:field="navigationHistory"/>
            </div>

            <div class="col-md-12" style="margin-top: 20px;">

                <div class="{css.style.pricingTable}">

                    <div class="{css.style.panel} panel-primary" style="border: none;">
                        <div class="controle-header panel-heading {css.style.panelHeadingLanding}">
                            <h1 class="panel-title {css.style.panelTitleLanding}">
                                Premium
                            </h1>
                        </div>
                        <div class="controle-panel-heading {css.style.panelHeadingLandingBox} panel-heading ">
                            Best Choice
                        </div>
                        <div class="panel-body {css.style.panelBodyLanding}">
                            <table class="table">
                                <tr>
                                    <td width="50px">
                                        <i class="fa fa-check"></i>
                                    </td>
                                    <td>30+ Accounts</td>
                                </tr>
                                <tr>
                                    <td width="50px">
                                        <i class="fa fa-check"></i>
                                    </td>
                                    <td>150+ Projects</td>
                                </tr>
                                <tr>
                                    <td width="50px">
                                        <i class="fa fa-check"></i>
                                    </td>
                                    <td>Lead Required</td>
                                </tr>
                            </table>
                        </div>
                        <div class="panel-footer {css.style.panelFooterLanding}">
                            <b:Button ui:field="testButton" color="#ffffff">Select</b:Button>
                            <!--<a href="#" class="btn {css.style.btn-price} btn-success btn-lg">SELECT</a>-->
                        </div>
                    </div>
                </div>
            </div>
        </div>


    </g:HTMLPanel>
</ui:UiBinder>
