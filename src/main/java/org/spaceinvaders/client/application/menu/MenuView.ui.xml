<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:b='urn:import:org.gwtbootstrap3.client.ui'
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html">

    <ui:with field="css" type="org.spaceinvaders.client.resources.AppResources"/>
    <ui:with field="nameTokens" type="org.spaceinvaders.client.place.NameTokens"/>

    <g:HTMLPanel>
        <b:Navbar ui:field="navBar">
            <b:NavbarHeader>
                <b:NavbarBrand ui:field="navbarBrand" targetHistoryToken="{nameTokens.getHome}">
                    Notus
                </b:NavbarBrand>
            </b:NavbarHeader>

            <b:NavbarCollapse>
                <b:NavbarNav>
                    <b:ListDropDown>
                        <b:AnchorButton dataToggle="DROPDOWN">Session</b:AnchorButton>
                        <b:DropDownMenu ui:field="dropDownMenu"/>
                    </b:ListDropDown>
                </b:NavbarNav>

                <b:Nav pull="RIGHT">
                    <b:NavbarText>
                        <b.html:Span ui:field="spanUsername"/>
                    </b:NavbarText>
                    <b:NavbarText>
                        <b:NavbarLink ui:field="navBarDisconnect">DÃ©connexion</b:NavbarLink>
                    </b:NavbarText>
                </b:Nav>

            </b:NavbarCollapse>

        </b:Navbar>
    </g:HTMLPanel>

</ui:UiBinder>