<?xml version='1.0' encoding='UTF-8'?>
<module rename-to='Notus'>

    <!-- Inherit from the Web Toolkit core. -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.google.gwt.inject.Inject'/>
    <inherits name="org.gwtbootstrap3.GwtBootstrap3"/>


       <inherits name="com.googlecode.gwt.charts.Charts"/>


    <!-- Rest dispatch -->
    <inherits name="com.gwtplatform.dispatch.rest.DispatchRest"/>
    <inherits name="com.gwtplatform.dispatch.rest.delegates.ResourceDelegate"/>

    <!-- Other module inherits -->
    <inherits name='com.gwtplatform.dispatch.Dispatch'/>
    <inherits name='com.gwtplatform.mvp.MvpWithEntryPoint'/>
    <inherits name="com.google.gwt.query.Query"/>
    <inherits name="com.googlecode.gwt.charts.Charts"/>
    <inherits name="com.arcbees.gquery.tooltip.Tooltip"/>

    <source path='client'/>
    <source path='shared'/>

    <source path="css/Tooltip.gss"/>

    <set-configuration-property name="CssResource.enableGss" value="true"/>
    <set-configuration-property name="CssResource.legacy" value="true"/>

    <!--TODO Remove for release-->
    <set-property name="user.agent" value="safari"/>

    <extend-configuration-property name="gin.ginjector.modules"
                                   value="org.spaceinvaders.client.gin.ClientModule"/>
</module>
